<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="team.lte.businessquery.mapper.KpiMapper">

    <select id="listData" resultType="team.lte.businessquery.entity.vo.TimeObject">
        select start_time, ${field} as result
        from tbkpi
        where sector_name = #{sectorName}
          and str_to_date(start_time, '%m/%d/%Y %H:%i:%S') between #{begin} and #{end};
    </select>
    <select id="listSectors" resultType="java.lang.String">
        select distinct sector_name
        from tbkpi;
    </select>
</mapper>
